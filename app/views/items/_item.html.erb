<br>

<table class="table table-bordered">
    <tr>
    <th>Description</th>
    <th>Days left</th>
    <th class='col-md-1'>Complete</th>
    </tr> 
    
    <% items.each do |item| %>
    <tr>
      <td> <%= item.name %> </td>
      <td> <%= distance_of_time_in_words_to_now(item.days_left)%> </td>
      <td> <%= link_to "Complete", [list, item], method: :delete, class: 'glyphicon glyphicon-ok' %> </td>
    </tr>
    <% end %>
 
</table>
